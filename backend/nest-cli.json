{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      {
        "include": "sales-documents/helpers/fonts/**/*",
        "outDir": "dist/src"
      },
      {
        "include": "sales-documents/helpers/output.css",
        "outDir": "dist/src"
      }
    ],
    "watchAssets": true
  }
}